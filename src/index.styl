@import './styles/*'
@import './components/**/*'
html, body
  margin 0
  font-family sans-serif
  height 100%
  min-height 100%
  display flex
  flex-direction column
  background #eeefef
  color #222223
  //overflow hidden
  sub
    vertical-align baseline
  img
    border-radius .3em
  input, button, textarea {
    -webkit-appearance none
    -webkit-border-radius 0px
    appearance none
    border-radius 0px
  }
  &.uploading
    input[type=submit]
      opacity 0.5
      pointer-events none

  &.loading
    .body
      opacity 0.2
  .body
    flex 1
    //overflow hidden
    table
      tr
        td
          vertical-align top
  .nav
    &.mobile
      display flex
      background #eeefef
      button
        margin 0
        flex 1
        padding 1em 0
        font-size 1.2em
        display flex
        align-items center
        justify-content center
        .icon
          width 1.9em
          height 1.9em
          margin-right .2em
        &.flex-2
          flex 2
      @media screen and (max-width: 20em)
        button
          span
            display none
  form
    .error
      color red
      display none
    &.submitted
      .error.invalid
        display block
  .simulator
    .output
      white-space pre
  /*
  .signin
    .new
      display none*/
  .schedulePercent-box
    border 1px solid black
    position relative
    height .6em
    background darkgrey
    overflow hidden
    .schedulePercent
      position absolute
      top 0
      left 0
      height 100%
      background white
      border-right 1px solid black
      &.danger
        background #ab3333
      &.warning
        background #ab7b33
      &.ok
        background #a4ab33
      &.good
        background #5cab33
.farm-logo
  background-image: url("data:image/svg+xml;base64,");
  width 2em
  height 2em
  background-size cover
  margin-right .5em
  &.white
    background-image url("data:image/svg+xml;base64,")
.icon-profile
  background-image: url("data:image/svg+xml;base64,")
  width 1.5em
  height 1.5em
  background-size cover
  margin-left .5em
